{
  "name": "Disco Test",
  "version": "1.0.0",
  "description": "test task",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "cookies",
    "webNavigation",
    "webRequest",
    "<all_urls>",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["<all_urls>"],
      "all_frames": true
    }
  ]
}
